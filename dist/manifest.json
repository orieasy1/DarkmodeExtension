{
  "name": "IXLAB Darkmode Extension",
  "version": "1.0.0",
  "description": "A Darkmode Chrome extension.",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
